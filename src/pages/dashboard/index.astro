---
import Layout from '@layouts/Layout.astro';
import { UserProfile } from 'astro-clerk-auth/components/react';
import { ClerkLayout } from 'astro-clerk-auth/components/control';
---

<ClerkLayout>
  <Layout title="Welcome to Astro.">
    <main>
      <p
        class="mt-2 text-4xl font-bold tracking-tight text-center text-gray-900 sm:text-5xl"
      >
        Dashboard
      </p>
      <section class="flex flex-1items-center justify-center">
        <UserProfile />
      </section>
    </main>
  </Layout>
</ClerkLayout>
